---
project:
  server:
    application:
      name: www.{{app.url}}
      alias: www.{{app.url}}
    database:
      name: db.{{app.url}}
      alias: db.{{app.url}}
      path: /usr/share/adminer/adminer/
    log_php: /var/log/php5/
    admin: support@{{app.url}}
  repository:
    url: https://'{{github_account}}':'{{github_password}}'@github.com/Company/Project.git
    version: branch

# Here you can set the content security policy of your production servers
security:
  allow:
    script:
      -
    style:
      -
    img:
      -
    font:
      -
